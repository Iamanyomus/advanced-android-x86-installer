if(e_CtrlID == CTRL_COMBOBOX_01) then
	-- the control message is from the select drive combo box...

	-- if it's an OnSelChanged message (i.e. the drive has changed)
	-- recalculate the space available on the selected drive
	if (e_MsgID == MSGID_ONSELCHANGED) then
		
		-- from On Preload:
		-- calculate the free space on the selected drive and update the size messages
		SelectDriveScreen.UpdateSizeMessages();
		
