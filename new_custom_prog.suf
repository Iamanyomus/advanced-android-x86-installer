<Event>
<Name>On Start</Name>
<Args/>
<Script>-- These actions are performed when the screen is shown.

-- Note: this is normally where you would put the actions
--       that you want this screen to perform (i.e. the
--       actions that you want to show the progress of).

-- Tip: use the DlgProgressBar actions (such as SetRange,
--		SetPos and Step) to make the progress bars reflect
--		the current status of your actions.


-- Define functions


function doNothin()
	return 0;
end

function updateProgStat(progString)
	
	DlgProgressBar.SetProperties(CTRL_PROGRESS_BAR_01, {Text=progString});

end	


function verifyInstallSource(toCheck, basename)

	 while ( File.DoesExist(toCheck) == false )
	 do
		if ( is_ntfs == false ) then
			local err_msg = basename.." is missing or failed to copy. Sometimes it is because your EXT type partition is not mounted as r/w or in panic-mode, you should RESTART and try again but skip partitioning step."
		else
			local err_msg = basename.." is missing or failed to copy."
		end
		choice = Dialog.Message("Notice", err_msg, MB_ABORTRETRYIGNORE, MB_ICONINFORMATION, MB_DEFBUTTON1);
		
		if ( choice == IDABORT ) then
			cleanTemp();
			Application.Exit(EXIT_REASON_USER_ABORTED);
			
		elseif ( choice == IDIGNORE ) then
			FILE_IGNORED = true;
			break;
		end
		
	end

end


function installFile(inputFile)

	updateProgStat(inputFile);	
	
	if ( loadFromISO == true ) then
	
		if ( inputFile == "system.img" ) or ( inputFile == "system.sfs" ) then
		
			sevenZip(ISO_IMAGE[1], SessionVar.Expand("%AppFolder%\\"), "", "system.*", true);
			
			-- State actual system image name after extraction
			if ( File.DoesExist(SessionVar.Expand("%AppFolder%\\system.img")) == true ) then
				inputFile = "system.img"
			else
				inputFile = "system.sfs"
			end
			
			-- A workaround to verify if the OS files we're acutally installed in AppFolder from ISO by only verifying system image. (Check existence and.or file size &lt; 100MB)
			if ( File.DoesExist(SessionVar.Expand("%AppFolder%\\")..inputFile) == false ) or ( File.GetSize(SessionVar.Expand("%AppFolder%\\")..inputFile) &lt; 104857600 ) then
				Dialog.Message("Error", "Failed to properly install files from your ISO, the installation can not continue.\r\nMake sure the installation partition has enough space and is writable.\r\nSometimes RESTARTING also resolves some weird issues.", MB_OK, MB_ICONINFORMATION, MB_DEFBUTTON1);
				cleanTemp();
				Application.Exit(1);
			end
			
		else
		
			sevenZip(ISO_IMAGE[1], SessionVar.Expand("%AppFolder%\\"), "", inputFile, true);

		end
		
		-- Verify if file was successfully installed for ISO also
		if ( inputFile == "gearlock" ) or ( inputFile == "ramdisk.img" ) then
			doNothin();
		else
			verifyInstallSource(SessionVar.Expand("%AppFolder%\\")..inputFile, inputFile);
		end
		
	else
	
		from = _SourceFolder.."\\"..inputFile;
		to = SessionVar.Expand("%AppFolder%\\")..inputFile;
	
		--- Check for file existence
		if ( inputFile == "gearlock" ) or ( inputFile == "ramdisk.img" ) then
			doNothin();
		else
			verifyInstallSource(from, inputFile);
		end
		
		-- Check if the user ignored it on missing
		if ( FILE_IGNORED == true ) then
			FILE_IGNORED = false;
			return 0;
		end
		
		if ( File.DoesExist(from) == true ) then
			retcode = Shell.Execute("C:\\Windows\\System32\\xcopy.exe", "open", from.." "..SessionVar.Expand("%AppFolder%\\").." /Y /Q", "", SW_HIDE, true);
			-- If an error occurred, display the error message.
			if (retcode ~= 0) then
				Dialog.Message("Error", "Failed to install "..inputFile..", the installation can not continue.\r\nMake sure the installation partition has enough space and is writable.\r\nSometimes RESTARTING also resolves some weird issues.", MB_OK, MB_ICONINFORMATION, MB_DEFBUTTON1);
				cleanTemp();
				Application.Exit(1);	
			end
		end
		
	end

end

-- Set total range
	DlgProgressBar.SetRange(CTRL_PROGRESS_BAR_01, 1, 100);
	

------- Install Grub2Win

	if ( install_grub2win == true ) then
	
		if ( File.DoesExist("C:\\grub2\\grub2win.exe") == true ) then updateProgStat("Updating Grub2Win"); else updateProgStat("Installing Grub2Win"); end;
		DlgProgressBar.SetPos(CTRL_PROGRESS_BAR_01, 10); installGrub2Win(); DlgProgressBar.SetPos(CTRL_PROGRESS_BAR_01, 20);
		
	else
	
		DlgProgressBar.SetPos(CTRL_PROGRESS_BAR_01, 20);
		
	end





------- Install basic Files
	FILE_IGNORED = false; -- Initialize ignored variable.
	installFile("initrd.img"); Application.Sleep(1000); DlgProgressBar.SetPos(CTRL_PROGRESS_BAR_01, 30);
	installFile("ramdisk.img"); Application.Sleep(1000); DlgProgressBar.SetPos(CTRL_PROGRESS_BAR_01, 40);
	installFile("kernel"); Application.Sleep(1000); DlgProgressBar.SetPos(CTRL_PROGRESS_BAR_01, 50);


------- Install GearLock
	installFile("gearlock");
	-- Check for bootinstall
	if ( File.DoesExist(SessionVar.Expand("%AppFolder%\\gearlock")) == false ) and ( File.DoesExist(SessionVar.Expand("%AppFolder%\\bootinstall-gearlock")) == false ) and ( gearlock_bootinstall == true ) then
		
		install_gearlock(SessionVar.Expand("%AppFolder%"), "no");

	end
	
	Application.Sleep(1000); DlgProgressBar.SetPos(CTRL_PROGRESS_BAR_01, 60);



------- Make data.img
	if ( genDataImg == true ) then
		if ( File.DoesExist("%AppFolder\\data.img%") == true ) then
			choice = Dialog.Message("Attention", "There is already a data.img present for your OS.\r\nDo you want to overwrite it ?", MB_YESNO, MB_ICONQUESTION, MB_DEFBUTTON2);
			if ( choice == IDYES ) then
				updateProgStat("Creating a "..genDataImgSize.."GB data.img");
				makeDataImg(genDataImgSize.."000", genDataImgSize.."GB");
			end
		else
			updateProgStat("Creating a "..genDataImgSize.."GB data.img");
			makeDataImg(genDataImgSize.."000", genDataImgSize.."GB");
		end
	end
	
	DlgProgressBar.SetPos(CTRL_PROGRESS_BAR_01, 75);



------- Install system image
	if ( File.DoesExist(_SourceFolder.."\\system.img") == true ) then
		installFile("system.img");
	else
		installFile("system.sfs")
	end
	
	DlgProgressBar.SetPos(CTRL_PROGRESS_BAR_01, 90);
	




------ Install shortcuts
	updateProgStat("Installing Shortcuts");
	-- Install program files
	Folder.Create("C:\\Program Files\\"..NAME);
	File.Install(SessionVar.Expand("%TempLaunchFolder%\\Boot.bat"), "C:\\Program Files\\"..NAME.."\\Boot.bat", FILE_INSTALL_ALWAYS, false, false, nil, nil);
	File.Install(SessionVar.Expand("%TempLaunchFolder%\\Run.bat"), "C:\\Program Files\\"..NAME.."\\Run.bat", FILE_INSTALL_ALWAYS, false, false, nil, nil);
	File.Install(LOGO_ICON, "C:\\Program Files\\"..NAME.."\\"..NAME..".ico", FILE_INSTALL_ALWAYS, false, false, nil, nil);
	File.Install(ASCII_ART, "C:\\Program Files\\"..NAME.."\\ascii.art", FILE_INSTALL_ALWAYS, false, false, nil, nil);
	
	-- Update NAME in shortcut source bat files
	TextFile.WriteFromString("C:\\Program Files\\"..NAME.."\\Run.bat", String.Replace(TextFile.ReadToString("C:\\Program Files\\"..NAME.."\\Run.bat"), "Android-x86-ASG", NAME, true), false);
	TextFile.WriteFromString("C:\\Program Files\\"..NAME.."\\Boot.bat", String.Replace(TextFile.ReadToString("C:\\Program Files\\"..NAME.."\\Boot.bat"), "Android-x86-ASG", NAME, true), false);
	
	-- Cleate shortcuts
	Shell.CreateShortcut(Shell.GetFolder(SHF_DESKTOP), "Grub2Win", "C:\\grub2\\grub2win.exe", "", "", "", 0, SW_SHOWNORMAL, nil, "", START_OPTION_DEFAULT);
	Shell.CreateShortcut(Shell.GetFolder(SHF_DESKTOP), "Boot "..NAME, "C:\\Program Files\\"..NAME.."\\Run.bat", "", "", "C:\\Program Files\\"..NAME.."\\"..NAME..".ico", 0, SW_SHOWNORMAL, nil, "supreme-gamers.com", START_OPTION_DEFAULT);
	
	-- Refresh the icon cache
	Application.Sleep(1500);
	Shell.Execute("C:\\Windows\\System32\\cmd.exe", "open", "/C ie4uinit.exe -ClearIconCache &amp; ie4uinit.exe -show", "", SW_HIDE, true);
	--File.Run("C:\\Windows\\System32\\cmd.exe", "/C taskkill /f /im explorer.exe &amp; del /a %userprofile%\AppData\Local\IconCache.db &amp; start explorer", "", SW_MINIMIZE, true);
	DlgProgressBar.SetPos(CTRL_PROGRESS_BAR_01, 95);




----- Process addtional files
	updateProgStat("Processing addtional files and folders");

	
	-- Create common dirs
	if ( is_ntfs == false ) and ( always_make_data_image == false ) then
		
		-- When EXT4
		File.Delete(SessionVar.Expand("%AppFolder%\\data.img"), false, false, false, nil);
		--File.Install(SessionVar.Expand("%TempLaunchFolder%\\check.me"), "%AppFolder%\\fixfs_firstboot", FILE_INSTALL_SAMEOLDER, false, false, nil, nil);
		
		if ( Folder.DoesExist(SessionVar.Expand("%AppFolder%\\data")) == false ) then
			Folder.Create(SessionVar.Expand("%AppFolder%\\data"));
		end
		
	else
		-- When NTFS
		if ( Folder.DoesExist(SessionVar.Expand("%AppFolder%\\data")) == true ) then
			Folder.DeleteTree(SessionVar.Expand("%AppFolder%\\data"));
		end
	
	end
	
	if ( Folder.DoesExist(SessionVar.Expand("%AppFolder%\\gearload")) == false ) and ( make_gearload_folder == true ) then
		Folder.Create(SessionVar.Expand("%AppFolder%\\gearload"));
	end
	
	
	if ( Folder.DoesExist(SessionVar.Expand("%AppFolder%\\system")) == true ) then
		Folder.Rename(SessionVar.Expand("%AppFolder%\\system"), SessionVar.Expand("%AppFolder%\\old.system"));
		Folder.DeleteTree(SessionVar.Expand("%AppFolder%\\old.system"), nil);
	end
	
	-- Create placeholder file for grub2win detection
	File.Install(SessionVar.Expand("%TempLaunchFolder%\\check.me"), SessionVar.Expand("%AppFolder%\\findme"), FILE_INSTALL_ALWAYS, false, false, nil, nil);
	
Application.Sleep(1500);
DlgProgressBar.SetPos(CTRL_PROGRESS_BAR_01, 100);</Script>
