function mountExt()

	if ( ext_is_mounted == false ) then
		
		ext_is_mounted = true;
		
		-- Start showing status dlg
		StatusDlg.Show(MB_ICONNONE, false);
		StatusDlg.SetTitle("Probing EXT4 Driver");
		StatusDlg.SetMessage("Trying to autoMount your EXT4 partitions ...");
		
		-- Kill if running
		StatusDlg.SetStatusText("Current Status: Killing GUI ExtFS handler");
		killProcess("ExtFS By SupremeGamers.exe"); Application.Sleep(1000);
		
		-- Force unMount to cleanly reMount
		StatusDlg.SetStatusText("Current Status: Force unMounting if is already mounted");
		umountExt();
		
		-- Start services
		StatusDlg.SetStatusText("Current Status: Starting services");
		Service.Start("DokanMounter", "DokanMounter", nil); Application.Sleep(3000);
		Service.Start("ParagonMounter", "ParagonMounter", nil); Application.Sleep(3000);
		StatusDlg.SetStatusText("Current Status: Final mountCall to System");
		Shell.Execute(paragon_mounter, "open", "/mount", "", SW_HIDE, false); Application.Sleep(9000);
		
		-- Finally hide statusDlg
		StatusDlg.Hide();
		Dialog.Message("Information", "If you can\'t find your EXT4 partitions then you must RESTART and try again.", MB_OK, MB_ICONINFORMATION, MB_DEFBUTTON1);
		
	end
	
end

function umountExt()

	if ( ext_is_mounted == true ) then
		get_paragon_mounter();
		Shell.Execute(paragon_mounter, "open", "/umount", "", SW_HIDE, true);
		Service.Stop("ParagonMounter", "ParagonMounter", 5);
		Service.Stop("DokanMounter", "DokanMounter", 5);
	end

end

function makeDataImg(sizeMb, sizeGb)

	sizeBytes = sizeMb * 1048576;
	Shell.Execute(_TempLaunchFolder.."\\make_ext4fs.exe", "open", " -l "..sizeBytes.." -b 4096 -a /data -L data "..SessionVar.Expand("%AppFolder%\\data.img"), _TempLaunchFolder, SW_HIDE, true);
	
end




---- HTTPS Download functions

-- Callback function for HTTP.DownloadSecure
function DownloadCallback(nDownloaded, nTotal, TransferRate, SecondLeft, SecondsLeftFormat, Message)
    -- Convert total and downloaded bytes into formatted strings
    sDownloaded = String.GetFormattedSize(nDownloaded, FMTSIZE_AUTOMATIC, true);
    sTotal = String.GetFormattedSize(nTotal, FMTSIZE_AUTOMATIC, true);

    -- Output time left, formatted.
    StatusDlg.SetMessage(dwFileName);
    
    -- Output formatted sizes to user through statusdlg status text
    StatusDlg.SetStatusText("Downloaded: " .. sDownloaded .. " / " .. sTotal.."   Time Left: "..SecondsLeftFormat);

    -- Set meter position (fraction downloaded * max meter range)
    StatusDlg.SetMeterPos((nDownloaded / nTotal) * 65534);

    -- Continue with the download.
    return true;
end
	
function downloadFile(dwUrl, localFile, name)
	downloadFileFailed = false -- reinitialize the check var
	while ( 1 == 1 )
	do		
		-- Set meter range (max range = 65534)
		StatusDlg.SetMeterRange(0, 65534);
		
		-- Show the StatusDlg
		StatusDlg.Show(0, false);
		StatusDlg.SetTitle("Downloading");
		StatusDlg.SetMessage("");
		StatusDlg.SetStatusText("");
			
		-- Set status file name
		dwFileName = name;
		
		-- Download a file from the internet to the user's computer
		-- Uses DownloadCallback() as the callback function
	
		HTTP.DownloadSecure(dwUrl, localFile, MODE_BINARY, 20, 443, nil, nil, DownloadCallback);
		
		-- Hide the StatusDlg
		StatusDlg.Hide();
		
		-- Handle error	
		error = Application.GetLastError();
		if ( error ~= 0 ) or ( File.DoesExist(localFile) == false ) then choice = Dialog.Message("Error during download", _tblErrorMessages[error].."\r\n\r\nDo you want to retry ?", MB_YESNO, MB_ICONEXCLAMATION, MB_DEFBUTTON1); else break; end
		if ( choice == IDNO ) then downloadFileFailed = true; break; end	
	end
end
		

function getEncodedUrl(fileName)

    encodedUrl = String.Replace(fileName, "/", "%2F", false);
    return encodedUrl;

end




