<Script>-- These actions are triggered by the controls on the screen.

function updateConfig()

	NAME = String.Replace(SessionVar.Expand("%UserProductName%"), " ", "-", true);
	VERSION = String.Replace(SessionVar.Expand("%UserProductVer%"), " ", "-", true);

	-- ReSet vendor FUNCTION values to the checklists since they overwrite em.
	always_make_data_imageProp = INIFile.GetValue(CONFIG_INI, "FUNCTIONS", "ALWAYS_MAKE_DATA_IMAGE");
	install_grub2winProp = INIFile.GetValue(CONFIG_INI, "FUNCTIONS", "INSTALL_GRUB2WIN");
	make_gearload_folderProp = INIFile.GetValue(CONFIG_INI, "FUNCTIONS", "MAKE_GEARLOAD_FOLDER");
	gearlock_bootinstallProp = INIFile.GetValue(CONFIG_INI, "FUNCTIONS", "GEARLOCK_BOOTINSTALL");
	
	if not ( always_make_data_imageProp == "" ) then
		if ( always_make_data_imageProp == "true" ) then always_make_data_image = true; else always_make_data_image = false; end;
	else
		if ( loadFromISO == true ) then
			sevenZip(ISO_IMAGE[1], _TempLaunchFolder, "", "ramdisk.img", true);
			if ( File.DoesExist(_TempLaunchFolder.."\\ramdisk.img") == false ) then always_make_data_image = true; else File.Delete(_TempLaunchFolder.."\\ramdisk.img", false, false, false, nil); end;
		else
			if ( File.DoesExist(_SourceFolder.."\\ramdisk.img") == false ) then always_make_data_image = true; end;
		end
	end
	
	if not ( install_grub2winProp == "" ) then
		if ( install_grub2winProp == "true" ) then install_grub2win = true; else install_grub2win = false; end;
	else
		install_grub2win = install_grub2win_btn;
	end
	
	if not ( gearlock_bootinstallProp == "" ) then
		if ( gearlock_bootinstallProp == "true" ) then gearlock_bootinstall = true; else gearlock_bootinstall = false; end;
	else
		gearlock_bootinstall = gearlock_bootinstall_btn;
	end
	
	if not ( make_gearload_folderProp == "" ) then if ( make_gearload_folderProp == "true" ) then make_gearload_folder = true; else make_gearload_folder = false; end; end;
	

	initSessionVars(); -- Re-initialize session vars

end

if (e_MsgID == MSGID_CLICKED) then
	-- a control was clicked...

	if (e_CtrlID == CTRL_BUTTON_01) or (e_CtrlID == CTRL_BUTTON_02) then

		if (e_CtrlID == CTRL_BUTTON_01) then
			if ( pirate == true ) then
				is_ntfs = false; SessionVar.Set("%InsType%","extFS");
				updateConfig(); 
				if ( paragon_installed == false ) then installParagon(); end;
				Screen.Next();
			else
				Dialog.Message("Info", "Not available for non-proprietary version of this installer", MB_OK, MB_ICONINFORMATION, MB_DEFBUTTON1);
			end

		elseif (e_CtrlID == CTRL_BUTTON_02) then
			is_ntfs = true; SessionVar.Set("%InsType%","otherFS");
			updateConfig();
			Screen.Jump("Data Image");
		end
		
	end

end
</Script>
