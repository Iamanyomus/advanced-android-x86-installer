<Script>-- These actions are performed when the Next button is clicked.

-- Store the selected drive letter (e.g. 'C:') in %SelectedDrive%
-- ...first, get the index of the selected item
local nSelected = DlgComboBox.GetProperties(CTRL_COMBOBOX_01).Selected;
-- ...then get the drive letter that was stored in the item's internal data field
local strDrive = DlgComboBox.GetItemData(CTRL_COMBOBOX_01, nSelected);
-- ...and store the drive letter in %SelectedDrive%
SessionVar.Set("%SelectedDrive%", strDrive);

-- advance to the next screen
if( File.DoesExist("%AppFolder%\\system.img") == true ) then
	Dialog.Message("Notice", "An earlier version of "..SessionVar.Expand("%ProductName%").." detected, your system.img will be updated !", MB_OK, MB_ICONNONE);
	--Folder.Rename("%AppFolder%\\system", "%AppFolder%\\old.system");
end

Screen.Next();</Script>
