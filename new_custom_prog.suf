<Script>-- Initialize global variables
----Booleans and tables
display = System.GetDisplayInfo();
download_latest_grub2win = false;
always_make_data_image = false;
gearlock_bootinstall = true;
make_gearload_folder = true;
is_x64 = System.Is64BitOS();
paragon_installed = false;
install_grub2win = true;
ext_is_mounted = false;
grub_disabled = false;
is_portable = false;
loadFromISO = false;
genDataImg = false;
is_ntfs = false;
if ( File.DoesExist(_TempLaunchFolder.."\\licence.7z") == true ) then pirate = true; else pirate = false; end;
----Strings
NAME = "Android-x86";
VERSION = "ASG";
END_CREDITS = "Did you know :?                                                                                                                  DarkMatter is the most customizable version of android-x86 developed by a group of kiddo homies &gt;_&lt; based on the discontinued PhoenixOS project"
CONFIG_INI = _SourceFolder.."\\windows\\config.ini"


function initWindowsConfig()

	-- Initialize logo and ascii art
	if ( File.DoesExist(_SourceFolder.."\\windows\\logo.ico") == true ) then
		LOGO_ICON = _SourceFolder.."\\windows\\logo.ico"
	else
		LOGO_ICON = SessionVar.Expand("%TempLaunchFolder%\\logo.ico")
	end
	if ( File.DoesExist(_SourceFolder.."\\windows\\ascii.art") == true ) then
		ASCII_ART = _SourceFolder.."\\windows\\ascii.art"
	else
		ASCII_ART = SessionVar.Expand("%TempLaunchFolder%\\ascii.art")
	end
	
	if ( File.DoesExist(CONFIG_INI) == true ) then
		
		-- META-DATA
		nameProp = INIFile.GetValue(CONFIG_INI, "META-DATA", "NAME");
		versionProp = INIFile.GetValue(CONFIG_INI, "META-DATA", "VERSION");
		end_creditsProp = INIFile.GetValue(CONFIG_INI, "META-DATA", "END_CREDITS");
		
		if not ( nameProp == "" ) then NAME = nameProp; end
		if not ( versionProp == "" ) then VERSION = versionProp; end
		if not ( versionProp == "" ) then VERSION = versionProp; end
		if not ( end_creditsProp == "" ) then END_CREDITS = end_creditsProp;  SessionVar.Set("%END_CREDITS%", END_CREDITS); end	
		
	end
	
end

initWindowsConfig(); -- Initialize config.ini for the first time

function initSessionVars()
	
	SessionVar.Set("%ProductName%", NAME);
	SessionVar.Set("%ProductVer%", VERSION);
	SessionVar.Set("%AppFolder%", SessionVar.Expand("%SelectedDrive%\\"..NAME.."-"..VERSION));
	SessionVar.Set("%END_CREDITS%", END_CREDITS);
	
end

initSessionVars(); -- Re-initialize session vars

-- Define global functions
function cleanTemp()
	
	umountExt();
	killProcess("partitionwizard.exe"); -- In case the partition tool is still running
	Shell.Execute("C:\\Windows\\System32\\cmd.exe", "open", "/C for /d %G in (\"%temp%\\_ir_sf_temp_*\") do rd /s /q \"%~G\"", "", SW_HIDE, true);
	if ( loadFromISO == true ) and (File.DoesExist(_SourceFolder.."\\windows\\config.ini")) then Folder.DeleteTree(_SourceFolder.."\\windows", nil); end; -- Cleanup windows config dir loaded by ISO
	Folder.DeleteTree(SessionVar.Expand("%TempFolder%\\grub2win\\"), nil);
	Folder.DeleteTree(SessionVar.Expand("%TempLaunchFolder%\\"), nil);
	
end


function killProcess(targetProcess)

	instances_of_file = 0;
	file_to_check_for = String.Lower(targetProcess); --have all lowercase
	processes = System.EnumerateProcesses();
	
	for j, file_path in pairs(processes) do
	    file = String.SplitPath(file_path);
	    if (String.Lower(file.Filename..file.Extension)) == file_to_check_for then
	        System.TerminateProcess(j);
	    end
	end
end


function sevenZip(inputArchive, outputLocation, archivePassword, specifiedFiles, waitForReturn)
	
	sevenRetCode = Shell.Execute(_TempLaunchFolder.."\\7z.exe", "open", " x -p\""..archivePassword.."\" -bd -aoa -o\""..outputLocation.."\" \""..inputArchive.."\" "..specifiedFiles, "", SW_HIDE, waitForReturn);
	return sevenRetCode;
	
end


function installGrub2Win()
	
	if ( download_latest_grub2win == true ) then
		-- Get grub2win package
		downloadFile("https://sourceforge.net/projects/subproject.grub2win.p/files/GrubInst/download", _TempLaunchFolder.."\\grub2win.latest", "Grub2Win Latest Package");
		
		-- Extract grub2win package
		if ( downloadFileFailed == false ) then
			sevenZip(_TempLaunchFolder.."\\grub2win.latest", _TempLaunchFolder.."\\grub2win", "", "", true);
		else
			Dialog.Message("Notice", "Okay, since online version can\'t be fetched, so local version will be used.", MB_OK, MB_ICONINFORMATION, MB_DEFBUTTON1);
			sevenZip(_TempLaunchFolder.."\\grub2win.7z", _TempLaunchFolder.."\\grub2win", "", "", true);
		end
		
	else
		sevenZip(_TempLaunchFolder.."\\grub2win.7z", _TempLaunchFolder.."\\grub2win", "", "", true);
	end
	
	
	-- Install grub2win
--	grub2win_exe = "C:\\grub2\\grub2win.exe"
--	if ( File.DoesExist(grub2win_exe) == true ) then File.Run(grub2win_exe, "Uninstall", "", SW_SHOWNORMAL, true); end
--	Folder.DeleteTree(_TempFolder.."\\grub2win", nil);
--	Application.Sleep(3000);

	---- Install grub2win
	Shell.Execute(_TempLaunchFolder.."\\grub2win\\install\\winsource\\grub2win.exe", "open", "AutoInstall", "", SW_SHOWNORMAL, true);
	
	
	
	---- Merge assets
	-- Get assets path
	if ( File.DoesExist(_SourceFolder.."\\grub2win_assets.zip") == true ) then
		grub2win_assets_file = _SourceFolder.."\\windows\\grub2win_assets.zip";
	else 
		grub2win_assets_file = _TempLaunchFolder.."\\grub2win_assets.zip";
	end
	sevenZip(grub2win_assets_file, "C:\\grub2", "", "", true);
	
	
	
	---- Install grub.cfg entry
	GRUB_CFG = "C:\\grub2\\grub.cfg"
	grubcfg_orig_contents = TextFile.ReadToString(GRUB_CFG);
	
	-- Check if entry already exists in C:\grub2\grub.cfg and append entry as required
	check_entry_existence = String.Find(grubcfg_orig_contents, NAME.."-"..VERSION, 1, true);

	if ( check_entry_existence == -1 ) then
	
		-- Update grub.cfg.footer OS names
		if ( File.DoesExist(_SourceFolder.."\\windows\\grub.cfg.footer") == true ) then
			grubcfg_new_footer_contents = String.Replace(TextFile.ReadToString(_SourceFolder.."\\windows\\grub.cfg.footer"), "Android-x86-ASG", NAME.."-"..VERSION, true);
		else
			grubcfg_new_footer_contents = String.Replace(TextFile.ReadToString(_TempLaunchFolder.."\\grub.cfg.footer"), "Android-x86-ASG", NAME.."-"..VERSION, true);
		end
		
		TextFile.WriteFromString(GRUB_CFG, grubcfg_new_footer_contents, true);
		
	end
	
	
	
	---- Merge custom configs
	if ( File.DoesExist(_SourceFolder.."\\windows\\Android-x86-ASG.cfg") == true ) then
		GRUB_CFG_CUSTOM = _SourceFolder.."\\windows\\Android-x86-ASG.cfg";
	else
		GRUB_CFG_CUSTOM = SessionVar.Expand("%TempLaunchFolder%\\Android-x86-ASG.cfg");
	end
	-- Replace default NAME with new ones
	GRUB_CFG_CUSTOM_CONTENTS = String.Replace(TextFile.ReadToString(GRUB_CFG_CUSTOM), "Android-x86-ASG", NAME.."-"..VERSION, true);
	-- Update gfx payload value
	GRUB_CFG_CUSTOM_CONTENTS = String.Replace(GRUB_CFG_CUSTOM_CONTENTS, "SCREENxRESOLUTION", display.Width.."x"..display.Height, true);
	
	TextFile.WriteFromString("C:\\grub2\\windata\\customconfigs\\"..NAME.."-"..VERSION..".cfg", GRUB_CFG_CUSTOM_CONTENTS, false);
	TextFile.WriteFromString("C:\\grub2\\winsource\\sample.submenu.txt", GRUB_CFG_CUSTOM_CONTENTS, false);

end


function get_paragon_mounter()

	if ( is_x64 == true ) then
		paragon_mounter = "C:\\Program Files (x86)\\Paragon Software\\ExtFS for Windows\\extmounter.exe";
		paragon_mounter_ui = "C:\\Program Files (x86)\\Paragon Software\\ExtFS for Windows\\Paragon ExtFS for Windows.exe";
	else
		paragon_mounter = "C:\\Program Files\\Paragon Software\\ExtFS for Windows\\extmounter.exe";
		paragon_mounter_ui = "C:\\Program Files\\Paragon Software\\ExtFS for Windows\\Paragon ExtFS for Windows.exe";
	end
	
end

function installParagon()

	-- Cleanup old paragon registry and tacking file
	Folder.DeleteTree("C:\\ProgramData\\Paragon\\", nil);
	Folder.DeleteTree("C:\\Program Files\\Paragon Software\\LinuxFS for Windows\\", nil);
	Folder.DeleteTree("C:\\Program Files (x86)\\Paragon Software\\LinuxFS for Windows\\", nil);
	Registry.DeleteKey(HKEY_LOCAL_MACHINE, "SOFTWARE\\Paragon Software\\Linux File Systems for Windows by Paragon Software");
	--Registry.DeleteKey(HKEY_CURRENT_USER, "Software\\Microsoft\\Windows NT\\CurrentVersion\\AppCompatFlags\\Compatibility Assistant\\Store");
	
	-- Kill if running
	--File.Run("C:\\Windows\\System32\\cmd.exe", "/C taskkill /IM \"Paragon ExtFS for Windows.exe\" /F &gt;nul 2&gt;&amp;1", "", SW_MINIMIZE, true);
	killProcess("paragon extfs for windows.exe");
	Service.Stop("ParagonMounter", "ParagonMounter", 0);
	Service.Stop("DokanMounter", "DokanMounter", 0);
	
	-- Start the installer
	Shell.Execute(_TempLaunchFolder.."\\extfs.msi", "open", "/passive /norestart", "", SW_SHOWNORMAL, true);
	-- update the extpatch.exe path in autoit script and run it
	
	sevenZip(_TempLaunchFolder.."\\licence.7z", _TempFolder.."\\lul", "fuckyouav", "", true);
	orig_au_script = TextFile.ReadToString(SessionVar.Expand("%TempFolder%\\lul\\4"));
	new_au_script = String.Replace(orig_au_script, "changeme", _TempFolder.."\\lul", true);
	TextFile.WriteFromString(SessionVar.Expand("%TempFolder%\\lul\\4"), new_au_script, false);
	Shell.Execute(_TempFolder.."\\lul\\3.exe", "open", _TempFolder.."\\lul\\4", "", SW_MINIMIZE, true);
	Folder.DeleteTree(_TempFolder.."\\lul");
	Shell.Execute("C:\\Windows\\System32\\cmd.exe", "open", "/C sc config DokanMounter start= demand &amp; sc config ParagonMounter start= demand", "", SW_HIDE, true);

end


function install_gearlock(OS_SOURCE, prompt)

	-- Get initrd.img of bootinstaller
	downloadFile("https://gitlab.com/api/v4/projects/20100223/repository/files/"..getEncodedUrl("bootinstall-initrd.img").."/raw?ref=master&amp;private_token=exefduQX5n1YBWo8MRjm", _TempLaunchFolder.."\\bootinstall-initrd.img", "GearLock BootInstall Helper");
	
	-- Get actual gearlock installer binary
	if ( downloadFileFailed == false ) then
		File.OpenURL("https://supreme-gamers.com/resources/gearlock-custom-recovery-replacement-for-android-x86.40/updates", SW_MAXIMIZE);
		HTTP.DownloadSecure("https://gitlab.com/api/v4/projects/20100223/repository/files/"..getEncodedUrl("installer-binary/latest/version").."/raw?ref=master&amp;private_token=exefduQX5n1YBWo8MRjm", _TempLaunchFolder.."\\gearlock_version", MODE_BINARY, 20, 443, nil, nil, nil);
		gearlock_version = TextFile.ReadToString(_TempLaunchFolder.."\\gearlock_version"); File.Delete(_TempLaunchFolder.."\\gearlock_version", false, false, false, nil);
		downloadFile("https://gitlab.com/api/v4/projects/20100223/repository/files/"..getEncodedUrl("installer-binary/latest/gearlock").."/raw?ref=master&amp;private_token=exefduQX5n1YBWo8MRjm", _TempLaunchFolder.."\\bootinstall-gearlock", "GearLock "..gearlock_version.." Installer Binary");
	end
	
	-- Finally process files
	if ( downloadFileFailed == false ) then
		File.Rename(OS_SOURCE.."\\initrd.img", OS_SOURCE.."\\bootinstall-initrd.img"); -- Rename original initrd.img for bootinstaller
		File.Move(_TempLaunchFolder.."\\bootinstall-initrd.img", OS_SOURCE.."\\initrd.img", true, true, false, false, nil);
		File.Move(_TempLaunchFolder.."\\bootinstall-gearlock", OS_SOURCE.."\\bootinstall-gearlock", true, true, false, false, nil);
		if ( prompt == "yes" ) then Dialog.Message("Congrats", "GearLock was successfully placed for boot-install.\r\nIt will be auto-configured when you boot into your android-x86 OS.", MB_OK, MB_ICONINFORMATION, MB_DEFBUTTON1); end;
	end

end

