<Name>On Next</Name>
<Args/>
<Script>-- These actions are performed when the Next button is clicked.
-- advance to the next screen

--Check for system files in SourceFolder
if ( File.DoesExist(_SourceFolder.."\\initrd.img") == false ) then
	answer = Dialog.Message("Attention", "No Android-x86 OS files were found in this folder.\r\nDo you want to select an OS-PACKAGE to load from it ?", MB_YESNO, MB_ICONINFORMATION, MB_DEFBUTTON1);
	if ( answer == IDYES ) then
		ISO_IMAGE = Dialog.FileBrowse(true, "Select Operating System Image / Archive", _SourceFolder, "ISO &amp; Archives (.iso, .zip, .7z, .rar)|*.iso;*.zip;*.7z;*.rar", "", "", false, true);
		
		if (ISO_IMAGE[1] ~= "CANCEL") and (ISO_IMAGE ~= nil) then
			loadFromISO = true;
			---- Try to load windows config even from ISO
			if (File.DoesExist(_SourceFolder.."\\windows\\config.ini")) then Folder.DeleteTree(_SourceFolder.."\\windows", nil); end;
			sevenZip(ISO_IMAGE[1], _SourceFolder, "", "windows", true);
			initWindowsConfig(); -- Re-initialize windows config
			initSessionVars(); -- Re-initialize session vars
		else
			Dialog.Message("Notice", "No valid input was provided, the installer will exit", MB_OK, MB_ICONINFORMATION, MB_DEFBUTTON1);
			Application.Exit(1);
		end
		
	else
		Dialog.TimedMessage("Notice", "Okay, then the installer has nothing to work with, thus it will exit", 2000, MB_ICONEXCLAMATION);
		Application.Exit(1);
	end
end


Screen.Jump("InstallationConfig");
</Script>
