<Script>-- These actions are triggered by the controls on the screen.

if (e_MsgID == MSGID_CLICKED) then

	if (e_CtrlID == CTRL_BUTTON_01) then
	
		Dialog.Message("Information", "If you can\'t find the boot entry for Android-x86 then follow these steps &gt;&gt;\r\n\r\n1..] Open \"Grub2Win\" from desktop\r\n\r\n2..] Click on \"Manage Boot Entries\" in the bottom\r\n\r\n3..] Click on \"Add A New Entry\" on the top\r\n\r\n4..] Click on \"Type\" menu and select \"submenu\"\r\n\r\n5..] Then click on \"Load Sample Code\"\r\n\r\n6..] Finally click on \"Ok &gt; Apply &gt; Ok\"\r\n\r\n\r\nAnd just like that your problem should be hopefully fixed :)\r\n", MB_OK, MB_ICONINFORMATION, MB_DEFBUTTON1);
	
	elseif (e_CtrlID == CTRL_BUTTON_02) then
	
		Dialog.Message("Warning", "Manually installing GearLock from here is not recommended.\r\n\r\nIt will fail if your some of your OS files reside in the EFI partition.\r\n\r\nProceed at your own risk.", MB_OK, MB_ICONINFORMATION, MB_DEFBUTTON1);

		while ( 1 == 1 )
		do
			selOSdir = Dialog.FolderBrowse("Select your OS folder for manual GearLock installation", _DesktopFolder);
			if ( selOSdir ~= "CANCEL") then
				if ( File.DoesExist(selOSdir.."\\initrd.img") == true ) then
					if ( File.DoesExist(selOSdir.."\\gearlock") == true ) or ( File.DoesExist(selOSdir.."\\bootinstall-gearlock") == true ) then
						Dialog.Message("Hey there", "Seems like GearLock is already installed, if you want to update it then use the build-in updater instead", MB_OK, MB_ICONINFORMATION, MB_DEFBUTTON1);
					else
						install_gearlock(selOSdir, "yes");
					end
					break
				else
					Dialog.Message("Hey there", "Seems like the folder you selected doesn\'t contain any operating system files, please select again.", MB_OK, MB_ICONINFORMATION, MB_DEFBUTTON1);
				end
			else
				break
			end
		end

	elseif (e_CtrlID == CTRL_BUTTON_03) then
	
		if ( pirate == true ) then
			if ( paragon_installed == false ) then
				installParagon();
			end
			
			mountExt();
		else
			Dialog.Message("Info", "Not available for non-proprietary version of this installer", MB_OK, MB_ICONINFORMATION, MB_DEFBUTTON1);
		end
	
	elseif (e_CtrlID == CTRL_BUTTON_04) then
	
		if ( pirate == true ) then
			runPartitionTool();
		else
			Dialog.Message("Info", "Not available for non-proprietary version of this installer", MB_OK, MB_ICONINFORMATION, MB_DEFBUTTON1);
		end	
		
	elseif (e_CtrlID == CTRL_BUTTON_05) then
		
		if ( pirate == true ) then
			if ( paragon_installed == true ) then
				Dialog.Message("Notice", "EXT4 drivers are already installed", MB_OK, MB_ICONINFORMATION, MB_DEFBUTTON1);
			else
				installParagon();
			end
		else
			Dialog.Message("Info", "Not available for non-proprietary version of this installer", MB_OK, MB_ICONINFORMATION, MB_DEFBUTTON1);
		end
		
	elseif (e_CtrlID == CTRL_BUTTON_06) then

		installGrub2Win();
		
	elseif (e_CtrlID == CTRL_BUTTON_07) then
	
		Dialog.Message("Notice", "This manual data.img creation will be implemented in the next update", MB_OK, MB_ICONINFORMATION, MB_DEFBUTTON1);
		
	elseif (e_CtrlID == CTRL_BUTTON_08) then
	
		Dialog.Message("Notice", "This feature is to be implemented in the next update", MB_OK, MB_ICONINFORMATION, MB_DEFBUTTON1);
		
	end
	
end
</Script>
