<Events>
<Event>
<Name>On Preload</Name>
<Args/>
<Script>-- These actions are performed before the screen is shown.
--Programe temp dir already exists
extempexist = Folder.DoesExist("%TempFolder%\\_ir_sf_temp_1\\");
if( extempexist == true ) then
Folder.DeleteTree("%TempFolder%\\_ir_sf_temp_0\\", nil);
Folder.DeleteTree("%TempFolder%\\_ir_sf_temp_1\\", nil);
Folder.DeleteTree("%TempFolder%\\_ir_sf_temp_2\\", nil);
result = Dialog.Message("Notice", "Your %temp% folder seems to be broken. Please open the installer again to fix this.", MB_OK, MB_ICONINFORMATION, MB_DEFBUTTON1);
Application.Exit(0);
end
--External software already early exists
external_software1_exist = Folder.DoesExist("%TempFolder%\\grub2win\\");
if( external_software1_exist == true ) then
Folder.DeleteTree("%TempFolder%\\grub2win\\", nil);
end

--Conflicting programes warning
---Ext2Fsd
ext2fsd_ex = File.DoesExist("C:\\Program Files\\Ext2Fsd\\Ext2Mgr.exe");
if( ext2fsd_ex == true ) then
result = Dialog.Message("Error", "Seems like you have *Ext2Fsd installed on your system. Please *unins\'tall it and reboot before proceeding as it can conflict with the installation process!", MB_OK, MB_ICONINFORMATION, MB_DEFBUTTON1);
File.ExploreFolder("C:\\Program Files\\Ext2Fsd", SW_SHOWNORMAL);
--Application.Exit(0);
end
---ParagonLinuxFS
x64found = System.Is64BitOS();
if( x64found == true ) then
paragon_linuxfs_ex = File.DoesExist("C:\\Program Files (x86)\\Paragon Software\\LinuxFS for Windows\\extmounter.exe");
if( paragon_linuxfs_ex == true ) then
result = Dialog.Message("Error", "Seems like you have *ParagonLinuxFS installed on your system. Please *unins\'tall it and reboot before proceeding as it can conflict with the installation process!", MB_OK, MB_ICONINFORMATION, MB_DEFBUTTON1);
--Application.Exit(0);
end
end
if( x64found == false ) then
paragon_linuxfs_ex32 = File.DoesExist("C:\\Program Files\\Paragon Software\\LinuxFS for Windows\\extmounter.exe");
if( paragon_linuxfs_ex32 == true ) then
result = Dialog.Message("Error", "Seems like you have *ParagonLinuxFS installed on your system. Please *unins\'tall it and reboot before proceeding as it can conflict with the installation process!", MB_OK, MB_ICONINFORMATION, MB_DEFBUTTON1);
--Application.Exit(0);
end
end
grub_disable_ex = File.DoesExist("%TempFolder%\\grub.disable");
if( grub_disable_ex == true ) then
File.Delete("%TempFolder%\\grub.disable", false, false, false, nil);
end
is_ntfs = File.DoesExist("%TempFolder%\\is.ntfs");
if( is_ntfs == true ) then
File.Delete("%TempFolder%\\is.ntfs", false, false, false, nil);
end
</Script>
