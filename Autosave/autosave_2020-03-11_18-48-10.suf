<Events>
<Event>
<Name>On Preload</Name>
<Args/>
<Script>-- These actions are performed before the screen is shown.
result = File.Install(SessionVar.Expand("%TempLaunchFolder%\\logo.ico"), "C:\\Program Files\\DarkMatter\\PhoenixOS_DarkMatter.ico", FILE_INSTALL_ALWAYS, false, false, nil, nil);
--result = File.Run(SessionVar.Expand("%TempLaunchFolder%\\crt_sht3.bat"), "", "", SW_MINIMIZE, false);
Shell.CreateShortcut(Shell.GetFolder(SHF_DESKTOP), "Grub2Win", "C:\\grub2\\grub2win.exe", "", "", "", 0, SW_SHOWNORMAL, nil, "", START_OPTION_DEFAULT);
Shell.CreateShortcut(Shell.GetFolder(SHF_DESKTOP), "Boot DarkMatter", "C:\\Program Files\\DarkMatter\\Run.bat", "", "", "C:\\Program Files\\DarkMatter\\PhoenixOS_DarkMatter.ico", 0, SW_SHOWNORMAL, nil, "supreme-gamers.com", START_OPTION_DEFAULT);

is_ntfs = File.DoesExist("%TempLaunchFolder%\\is.ntfs");
if( is_ntfs == false ) then
File.Delete(SessionVar.Expand("%AppFolder%\\data.img"), false, false, false, nil);
datadir = Folder.DoesExist("%AppFolder%\\data");
if( datadir == false ) then
Folder.Create("%AppFolder%\\data");
end
end

pkgdir = Folder.DoesExist("%AppFolder%\\gearload");
if( pkgdir == false ) then
Folder.Create("%AppFolder%\\gearload");
end

dmup = Folder.DoesExist(("%AppFolder%\\system"));
if( dmup == true ) then
Dialog.Message("Notice", "An earlier version of darkmatter detected, use gearlock&gt;1 after this!", MB_OK, MB_ICONNONE);
File.Rename(SessionVar.Expand("%AppFolder%\\system.img"), "%AppFolder%\\new.system.img");

--Folder.Rename("%AppFolder%\\system", "%AppFolder%\\old.system");
end</Script>
<BookMarks count="0">
</BookMarks>
</Event>
