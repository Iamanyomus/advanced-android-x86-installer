<Script>-- These actions are triggered by the controls on the screen.

if (e_MsgID == MSGID_CLICKED) then

	if (e_CtrlID == CTRL_BUTTON_01) then
	
		Dialog.Message("Information", "If you can\'t find the boot entry for PhoenixOS-DarkMatter then follow these steps &gt;&gt;\r\n\r\n1..] Open \"Grub2Win\" from desktop\r\n\r\n2..] Click on \"Manage Boot Entries\" in the bottom\r\n\r\n3..] Click on \"Add A New Entry\" on the top\r\n\r\n4..] Click on \"Type\" menu and select \"submenu\"\r\n\r\n5..] Then click on \"Load Sample Code\"\r\n\r\n6..] Finally click on \"Ok &gt; Apply &gt; Ok\"\r\n\r\n\r\nAnd just like that your problem should be hopefully fixed :)\r\n", MB_OK, MB_ICONINFORMATION, MB_DEFBUTTON1);
	
	elseif (e_CtrlID == CTRL_BUTTON_02) then
		Dialog.Message("Information", "If you face some error like \"Unknown Decompression error\" or if you can\'t find your ext4 partition in the installer then simply reboot and try again.", MB_OK, MB_ICONINFORMATION, MB_DEFBUTTON1);


	elseif (e_CtrlID == CTRL_BUTTON_03) then
	
		if ( paragon_installed == false ) then
			installParagon();
		end
		
		mountExt();
	
	elseif (e_CtrlID == CTRL_BUTTON_04) then
		
		runPartitionTool();
		
	elseif (e_CtrlID == CTRL_BUTTON_05) then
	
		if ( paragon_installed == true ) then
			Dialog.Message("Notice", "EXT4 drivers are already installed", MB_OK, MB_ICONINFORMATION, MB_DEFBUTTON1);
		else
			installParagon();
		end
		
	elseif (e_CtrlID == CTRL_BUTTON_06) then

		installGrub2Win();

	end

end
</Script>
<BookMarks count="0">
</BookMarks>
