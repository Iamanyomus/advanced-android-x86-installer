<Name>On Preload</Name>
<Args/>
<Script>-- These actions are performed before the screen is shown.

-- set up a table to contain this screen's functions (this is a nice way to avoid naming conflicts)
if not SelectDriveScreen then SelectDriveScreen = {}; end

-- This function calculates the free space on the currently selected drive 
-- and updates the 'Space required:' and 'Space available on selected drive:' messages
function SelectDriveScreen.UpdateSizeMessages()

	-- get the index of the selected item
	local nSelectedIndex = DlgComboBox.GetProperties(CTRL_COMBOBOX_01).Selected;
			
	-- get the corresponding drive letter, which we stored in the item data
	local strDrive = DlgComboBox.GetItemData(CTRL_COMBOBOX_01, nSelectedIndex);
			
	-- from _SUF70_Global_Functions.lua:
	-- calculate the amount of disk space available on the selected drive
	_SpaceAvailable = g_GetFreeSpaceInBytes(strDrive);

	-- set %SpaceAvailable% to a string with an appropriate unit of measurement (e.g. "0 bytes")
	SessionVar.Set("%SpaceAvailable%", String.GetFormattedSize(_SpaceAvailable) );

	-- from _SUF70_Global_Functions.lua:
	-- update the 'Space required:' and 'Space available on selected drive:' messages (expands any session variables in the bottom instructions text)
	g_UpdateStaticTextCtrl(CTRL_STATICTEXT_BOTTOMINSTRUCTIONS, "IDS_CTRL_STATICTEXT_BOTTOMINSTRUCTIONS");

end

-- calculate the amount of space required for the installation
_SpaceRequired = SetupData.CalculateRequiredSpace();

-- format it as a string with an appropriate unit of measurement (e.g. "0 bytes")
local strSpaceRequired = String.GetFormattedSize(_SpaceRequired);

-- store the string in a session variable so it can be used in the screen text
SessionVar.Set("%SpaceRequired%", strSpaceRequired);

-- from _SUF70_Global_Functions.lua:
-- fill a table with all fixed and remote drives
tbDrives = g_GetDriveLetters(DRIVE_FIXED, DRIVE_REMOTE);

-- from _SUF70_Global_Functions.lua:
-- add all of the drives in tbDrives to the combo box
g_FillComboBoxWithDriveDisplayNames(CTRL_COMBOBOX_01, tbDrives);

-- call our function to calculate the free space on the selected drive and update the size messages
SelectDriveScreen.UpdateSizeMessages();
</Script>
