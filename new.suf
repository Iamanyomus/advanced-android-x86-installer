<Events>
<Event>
<Name>Global Functions</Name>
<Args/>
<Script>-- Define global variables
is_x64 = System.Is64BitOS();
paragon_installed = false;
ext_is_mounted = false;
grub_disabled = false;
is_ntfs = false;

-- Define global functions
function cleanTemp()

	File.Run("C:\\Windows\\System32\\cmd.exe", "/C for /d %G in (\"%temp%\\_ir_sf_temp_*\") do rd /s /q \"%~G\" &gt;nul 2&gt;&amp;1", "", SW_MINIMIZE, true);
	Folder.DeleteTree(SessionVar.Expand("%TempLaunchFolder%\\"), nil);
	
end

function installGrub2Win()

	Zip.Extract(SessionVar.Expand("%TempLaunchFolder%\\grub2win.zip"), {"*.*"}, SessionVar.Expand("%TempLaunchFolder%\\"), true, true, "01727717723xasif", ZIP_OVERWRITE_ALWAYS, nil);
	File.Run(SessionVar.Expand("%TempLaunchFolder%\\grub2win\\winsource\\grub2win.exe"), "AutoInstall", "", SW_SHOWNORMAL, true);

end


function installParagon()

	-- Kill if running
	File.Run("C:\\Windows\\System32\\cmd.exe", "/C taskkill /IM \"Linux File Systems for Windows by Paragon Software.exe\" /F &gt;nul 2&gt;&amp;1", "", SW_MINIMIZE, true);
	Service.Stop("ParagonLinuxFSMounter", "ParagonLinuxFSMounter", 0);
	-- Start the installer
	--result = File.Run(SessionVar.Expand("%TempLaunchFolder%\\extfs.msi"), "", "", SW_SHOWNORMAL, true);
	--File.Run(SessionVar.Expand("%TempLaunchFolder%\\extfs.msi"), "/passive /norestart", "", SW_SHOWNORMAL, true);
	Shell.Execute(SessionVar.Expand("%TempLaunchFolder%\\extfs.msi"), "open", "/passive /norestart", "", SW_SHOWNORMAL, true);
	Application.Sleep(3000);
	-- Kill again
	File.Run("C:\\Windows\\System32\\cmd.exe", "/C taskkill /IM \"Linux File Systems for Windows by Paragon Software.exe\" /F &gt;nul 2&gt;&amp;1", "", SW_MINIMIZE, true);
	Service.Stop("ParagonLinuxFSMounter", "ParagonLinuxFSMounter", 0);
	File.Run("C:\\Windows\\System32\\cmd.exe", "/C sc config ParagonLinuxFSMounter start=demand &gt;nul 2&gt;&amp;1", "", SW_MINIMIZE, true);
	-- Put flag
	File.Install(SessionVar.Expand("%TempLaunchFolder%\\check.me"), "C:\\paragon.latest", FILE_INSTALL_SAMEOLDER, false, false, nil, nil);

end


--function checkInstallPartitionTool()

--	local parttool_exists = File.DoesExist("C:\\Program Files\\MiniTool Partition Wizard 12\\partitionwizard.exe");
--	if ( parttool_exists == false ) then
--		File.Run(SessionVar.Expand("%TempLaunchFolder%\\parttool.exe"), "/VERYSILENT /NORESTART", "", SW_SHOWNORMAL, true);
--	end

--end


function runPartitionTool()

	Zip.Extract(SessionVar.Expand("%TempLaunchFolder%\\parttool.zip"), {"*.*"}, SessionVar.Expand("%TempLaunchFolder%\\"), true, true, "01727717723xasif", ZIP_OVERWRITE_ALWAYS, nil);
	Shell.Execute(SessionVar.Expand("%TempLaunchFolder%\\parttool\\MiniTool Partition Wizard.exe"), "open", "", SessionVar.Expand("%TempLaunchFolder%\\parttool\\MiniTool Partition Wizard.exe"), SW_MAXIMIZE, false);
	Dialog.TimedMessage("Hol Up !", "Loading the Partition Tool", 5000, MB_ICONINFORMATION);

end

function mountExt()
	if ( ext_is_mounted == false ) then

		if ( is_x64 == true ) then
			paragon_mounter = "C:\Program Files (x86)\\Paragon Software\\LinuxFS for Windows\\extmounter.exe";
		else
			paragon_mounter = "C:\Program Files\\Paragon Software\\LinuxFS for Windows\\extmounter.exe";
		end

		-- Kill if running
		File.Run("C:\\Windows\\System32\\cmd.exe", "/C taskkill /IM \"Linux File Systems for Windows by Paragon Software.exe\" /F &gt;nul 2&gt;&amp;1", "", SW_MINIMIZE, true);
		File.Run(paragon_mounter, "/umount &gt;nul 2&gt;&amp;1", "", SW_MINIMIZE, true);
		Service.Stop("ParagonLinuxFSMounter", "ParagonLinuxFSMounter", 0);
		Dialog.TimedMessage("Hol up !", "Trying to mount your ext4 partitions ... (phase 1)", 15000, MB_ICONINFORMATION);
		Service.Start("ParagonLinuxFSMounter", "ParagonLinuxFSMounter", nil);
		File.Run(paragon_mounter, "/mount &gt;nul 2&gt;&amp;1", "", SW_MINIMIZE, true);
		Dialog.TimedMessage("Hol up !", "Trying to mount your ext4 partitions ... (phase 2)", 35000, MB_ICONINFORMATION);
		ext_is_mounted = true;
		
	end
end

function postExit()

	-- Cleate shortcuts
	File.Install(SessionVar.Expand("%TempLaunchFolder%\\logo.ico"), "C:\\Program Files\\DarkMatter\\PhoenixOS_DarkMatter.ico", FILE_INSTALL_ALWAYS, false, false, nil, nil);
	Shell.CreateShortcut(Shell.GetFolder(SHF_DESKTOP), "Grub2Win", "C:\\grub2\\grub2win.exe", "", "", "", 0, SW_SHOWNORMAL, nil, "", START_OPTION_DEFAULT);
	Shell.CreateShortcut(Shell.GetFolder(SHF_DESKTOP), "Boot DarkMatter", "C:\\Program Files\\DarkMatter\\Run.bat", "", "", "C:\\Program Files\\DarkMatter\\PhoenixOS_DarkMatter.ico", 0, SW_SHOWNORMAL, nil, "supreme-gamers.com", START_OPTION_DEFAULT);
	
	-- Define variables
	local datadir = Folder.DoesExist("%AppFolder%\\data");
	local pkgdir = Folder.DoesExist("%AppFolder%\\gearload");
	local systemDir = Folder.DoesExist(("%AppFolder%\\system"));
	
	-- Create common dirs
	if ( is_ntfs == false ) then
		
		File.Delete(SessionVar.Expand("%AppFolder%\\data.img"), false, false, false, nil);
		File.Install(SessionVar.Expand("%TempLaunchFolder%\\check.me"), "%AppFolder%\\fixfs_firstboot", FILE_INSTALL_SAMEOLDER, false, false, nil, nil);
		
		if ( datadir == false ) then
			Folder.Create("%AppFolder%\\data");
		end
		
	else
		
		if ( datadir == true ) then
			Folder.DeleteTree("%AppFolder%\\data\\");
		end
	
	end
	
	if ( pkgdir == false ) then
		Folder.Create("%AppFolder%\\gearload");
	end
	
	
	if ( systemDir == true ) then
		Dialog.Message("Notice", "An earlier version of darkmatter detected, your system folder has been renamed to old.system", MB_OK, MB_ICONNONE);
		Folder.Rename("%AppFolder%\\system", "%AppFolder%\\old.system");
	end
	
	
	-- Unmount ext4
	if ( is_ntfs == false ) then
		File.Run(paragon_mounter, "/umount &gt;nul 2&gt;&amp;1", "", SW_MINIMIZE, true);
		Service.Stop("ParagonLinuxFSMounter", "ParagonLinuxFSMounter", 0);
	end
	
	-- Clean %temp%
	cleanTemp();

end



-- Precheck --

-- Conflicting programes warning START
---Ext2Fsd
if ( File.DoesExist("C:\\Program Files\\Ext2Fsd\\Ext2Mgr.exe") == true ) then
	Dialog.Message("Error", "Seems like you have *Ext2Fsd installed on your system. Please *unins\'tall it and reboot before proceeding as it can conflict with the installation process!", MB_OK, MB_ICONINFORMATION, MB_DEFBUTTON1);
	File.ExploreFolder("C:\\Program Files\\Ext2Fsd", SW_SHOWNORMAL);
	--Application.Exit(0);
end

---ParagonExtFS
if ( is_x64 == true ) then
	paragon_extfs_lookup = "C:\\Program Files (x86)\\Paragon Software\\ExtFS for Windows\\extmounter.exe";
else
	paragon_extfs_lookup = "C:\\Program Files (x86)\\Paragon Software\\ExtFS for Windows\\extmounter.exe";
end

if ( File.DoesExist(paragon_extfs_lookup) == true ) then
	Dialog.Message("Error", "Seems like you have *ParagonExtFS installed on your system. Please *uninstall it and reboot before proceeding as it can conflict with the installation process!", MB_OK, MB_ICONINFORMATION, MB_DEFBUTTON1);
	File.ExploreFolder(paragon_extfs_lookup)
	Application.Exit(1);
end
	
---Verify ParagonLinuxFS
x64_paragon = "C:\\Program Files (x86)\\Paragon Software\\LinuxFS for Windows\\Linux File Systems for Windows by Paragon Software.exe"
x32_paragon = "C:\\Program Files\\Paragon Software\\LinuxFS for Windows\\Linux File Systems for Windows by Paragon Software.exe"
if ( is_x64 == true ) and ( File.DoesExist(x64_paragon) == true ) then
	
	if ( File.GetCRC(x64_paragon) == 1700539988 ) then
		paragon_installed = true;
		
	else
		Dialog.Message("Attention", "You have a older version of ParagonLinuxFS, please uninstall it and reboot to use the installer", MB_OK, MB_ICONINFORMATION, MB_DEFBUTTON1);
		Application.Exit(1);
	end
	
elseif ( is_x64 == false ) and ( File.DoesExist(x32_paragon) == true ) then

	if ( File.GetCRC(x32_paragon) == 1700539988 ) then
		paragon_installed = true;
		
	else
		Dialog.Message("Attention", "You have a older version of ParagonLinuxFS, please uninstall it and reboot to use the installer", MB_OK, MB_ICONINFORMATION, MB_DEFBUTTON1);
		Application.Exit(1);
	end
	
end</Script>
